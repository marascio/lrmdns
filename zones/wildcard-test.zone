$ORIGIN example.com.
$TTL 3600

@   IN  SOA ns1.example.com. admin.example.com. (
            2025120601 ; Serial
            7200       ; Refresh
            3600       ; Retry
            1209600    ; Expire
            86400      ; Minimum TTL
        )

@   IN  NS  ns1.example.com.
@   IN  NS  ns2.example.com.

ns1 IN  A   192.0.2.1
ns2 IN  A   192.0.2.2

; Wildcard records
*   IN  A   192.0.2.100
*   IN  TXT "Wildcard match"

; Specific record that should override wildcard
www IN  A   192.0.2.10
